{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "beerapp",
    "labels": {
      "app": "node"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "node"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "node"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "node",
            "image": "kalagounet/beerapp:1.0.1",
            "ports": [{ "containerPort": 5000 }]
          }
        ],
        "imagePullSecrets": [{ "name": "regcred" }]
      }
    }
  }
}
